
import sys
sys.stdin = open('input.txt', 'r')

T = int(input())
for tc in range(1,T+1):
    N = int(input())
    matrix = [list(map(int, input().split())) for _ in range(N)]

    total = 0
    
    for r in range(N):
        min_v = 100
        for c in range(N):
            min_v = matrix[r][c] if min_v > matrix[r][c] else min_v

        total += min_v

    print(f'#{tc} {total}')
